{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"moduleResolution": "node",
		"target": "esnext",
		"module": "esnext",
		"paths": {
			"@/*": [
				"src/*"
			],
			"@@/*": [
				".dumi/tmp/*"
			],
			"dumi": [
				"."
			],
			"dumi/theme/*": [
				"src/client/theme-default/*"
			],
			"sukee": ["components/index.tsx"],
			"sukee/es/*": ["components/*"],
			"sukee/lib/*": ["components/*"]
		},
		"typeRoots": [
			"node_modules/@types",
			"types.d.ts"
		]
	}
}
