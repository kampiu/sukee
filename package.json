{
	"name": "suko",
	"version": "0.0.3",
	"private": true,
	"description": "",
	"keywords": [],
	"homepage": "https://github.com/kampiu/sukee/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/kampiu/sukee.git"
	},
	"license": "MIT",
	"author": "kampiu <1479376017@qq.com>",
	"scripts": {
		"prebuild": "rimraf dist",
		"precommit": "lint-staged",
		"commit": "git-cz",
		"lint:script": "prettier --write ./src/**/*.ts && eslint --fix",
		"report-coverage": "cat ./coverage/lcov.info | coveralls",
		"start": "dumi dev",
		"docs:build": "dumi build",
		"test": "jest --coverage",
		"test:prod": "npm run lint && npm run test -- --no-cache",
		"test:watch": "jest --coverage --watch"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"{src,test}/**/*.ts": [
			"prettier --write ./src/**/*.ts",
			"eslint --fix",
			"git add"
		]
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-conventional-changelog"
		}
	},
	"jest": {
		"collectCoverageFrom": [
			"src/*.{js,ts}"
		],
		"coveragePathIgnorePatterns": [
			"/node_modules/",
			"/test/"
		],
		"coverageThreshold": {
			"global": {
				"branches": 90,
				"functions": 95,
				"lines": 95,
				"statements": 95
			}
		},
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js"
		],
		"testEnvironment": "node",
		"testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
		"transform": {
			".(ts|tsx)": "ts-jest"
		}
	},
	"devDependencies": {
		"@babel/cli": "^7.14.8",
		"@babel/core": "^7.15.0",
		"@babel/eslint-parser": "^7.15.0",
		"@babel/eslint-plugin": "^7.14.5",
		"@babel/plugin-proposal-class-properties": "^7.14.5",
		"@babel/plugin-proposal-decorators": "^7.14.5",
		"@babel/plugin-proposal-object-rest-spread": "^7.14.7",
		"@babel/plugin-transform-runtime": "^7.15.8",
		"@babel/preset-env": "^7.21.4",
		"@babel/preset-react": "^7.18.6",
		"@babel/preset-typescript": "7.15.0",
		"@babel/runtime": "^7.15.3",
		"@babel/types": "^7.21.5",
		"@commitlint/cli": "^9.1.2",
		"@commitlint/config-conventional": "^7.6.0",
		"@jest/globals": "^29.7.0",
		"@testing-library/dom": "^9.0.0",
		"@testing-library/jest-dom": "^5.17.0",
		"@testing-library/react": "^12.0.0",
		"@testing-library/react-hooks": "^8.0.1",
		"@testing-library/user-event": "^14.4.2",
		"@types/jest": "^29.5.10",
		"@types/jest-axe": "^3.5.9",
		"@types/jsdom": "^21.1.6",
		"@types/lodash": "^4.14.182",
		"@types/node": "^16.16.0",
		"@types/react": "^17.0.0",
		"@types/react-dom": "^17.0.0",
		"@vitejs/plugin-react": "^2.0.1",
		"autoprefixer": "^10.4.13",
		"babel": "^6.23.0",
		"babel-core": "^7.0.0-bridge.0",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.2.2",
		"babel-runtime": "^6.26.0",
		"clsx": "^2.1.0",
		"commitizen": "^3.0.0",
		"coveralls": "^3.0.2",
		"cross-env": "^5.2.0",
		"cz-conventional-changelog": "^2.1.0",
		"dumi": "^2.2.17",
		"dumi-assets-types": "^2.0.0-alpha.0",
		"eslint": "^8.17.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-typescript": "^2.7.1",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.5.1",
