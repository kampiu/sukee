{
	"name": "suko",
	"private": true,
	"version": "0.0.3",
	"description": "",
	"keywords": [],
	"author": "KAM <1479376017@qq.com>",
	"repository": {
		"type": "git",
		"url": "https://github.com/kampiu/sukee.git"
	},
	"license": "MIT",
	"engines": {
		"node": ">=16.0.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"sukee": "vite --mode development",
		"subuild": "vite build --mode production",
		"prebuild": "rimraf dist",
		"build": "tsc --module commonjs && rollup -c rollup.config.ts",
		"serve": "cross-env ENV=development rollup -c -w",
		"docs": "typedoc --out docs --target es6 --theme minimal --mode file src/suke",
		"start": "rollup -c rollup.config.ts -w",
		"test": "jest --coverage",
		"test:watch": "jest --coverage --watch",
		"test:prod": "npm run lint && npm run test -- --no-cache",
		"deploy-docs": "ts-node tools/gh-pages-publish",
		"report-coverage": "cat ./coverage/lcov.info | coveralls",
		"commit": "git-cz",
		"semantic-release": "semantic-release",
		"semantic-release-prepare": "ts-node tools/semantic-release-prepare",
		"precommit": "lint-staged",
		"lint:script": "prettier --write ./src/**/*.ts && eslint --fix",
		"travis-deploy-once": "travis-deploy-once",
		"major": "npm version major -m 'build: update major'",
		"minor": "npm version minor -m 'build: update minor'",
		"patch": "npm version patch -m 'build: update patch'",
		"pub": "npm run build && npm publish --access=public",
		"pub:major": "npm run major && npm publish --access=public",
		"pub:minor": "npm run minor && npm publish --access=public",
		"pub:patch": "npm run patch && npm publish --access=public"
	},
	"homepage": "https://github.com/kampiu/sukee/blob/master/README.md",
	"lint-staged": {
		"{src,test}/**/*.ts": [
			"prettier --write ./src/**/*.ts",
			"eslint --fix",
			"git add"
		]
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-conventional-changelog"
		}
	},
	"jest": {
		"transform": {
			".(ts|tsx)": "ts-jest"
		},
		"testEnvironment": "node",
		"testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js"
		],
		"coveragePathIgnorePatterns": [
			"/node_modules/",
			"/test/"
		],
		"coverageThreshold": {
			"global": {
				"branches": 90,
				"functions": 95,
				"lines": 95,
				"statements": 95
			}
		},
		"collectCoverageFrom": [
			"src/*.{js,ts}"
		]
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"devDependencies": {
		"@babel/eslint-parser": "^7.18.2",
		"@commitlint/cli": "^7.1.2",
		"@commitlint/config-conventional": "^7.1.2",
		"@types/jest": "^23.3.2",
		"@types/lodash": "^4.14.182",
		"@types/node": "^16.16.0",
		"@types/react": "^17.0.0",
		"@types/react-dom": "^17.0.0",
		"@types/rollup-plugin-json": "^3.0.3",
		"@types/rollup-plugin-node-resolve": "^4.1.0",
		"@types/rollup-plugin-sourcemaps": "^0.5.0",
		"@typescript-eslint/eslint-plugin": "^5.36.1",
		"@typescript-eslint/parser": "^5.36.1",
		"@vitejs/plugin-react": "^2.0.1",
		"autoprefixer": "^10.4.13",
		"babel": "^6.23.0",
		"babel-eslint": "^10.1.0",
		"colors": "^1.3.2",
		"commitizen": "^3.0.0",
		"coveralls": "^3.0.2",
		"cross-env": "^5.2.0",
		"cz-conventional-changelog": "^2.1.0",
		"eslint": "^8.23.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-typescript": "^3.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.31.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^1.0.1",
		"jest": "^23.6.0",
		"jest-config": "^23.6.0",
		"lerna": "4.0.0",
		"less": "^4.1.2",
		"lint-staged": "^8.0.0",
		"lodash": "^4.17.21",
		"prettier": "^2.6.2",
		"prompt": "^1.0.0",
		"react": "^17.0.0",
		"react-dom": "^17.0.0",
		"replace-in-file": "^3.4.2",
		"rimraf": "^2.6.2",
		"rollup": "^2.79.0",
		"rollup-plugin-babel": "^4.4.0",
		"rollup-plugin-commonjs": "^9.1.8",
		"rollup-plugin-json": "^3.1.0",
		"rollup-plugin-node-resolve": "^3.4.0",
		"rollup-plugin-serve": "^2.0.1",
		"rollup-plugin-sourcemaps": "^0.4.2",
		"rollup-plugin-typescript": "^1.0.1",
		"rollup-plugin-typescript2": "^0.18.1",
		"semantic-release": "^15.9.16",
		"shelljs": "^0.8.3",
		"travis-deploy-once": "^5.0.9",
		"ts-jest": "^23.10.2",
		"ts-node": "^7.0.1",
		"typedoc": "^0.23.14",
		"typescript": "^4.6.3",
		"vite": "^3.0.9",
		"vite-plugin-eslint": "^1.8.1"
	},
	"workspaces": [
		"packages/*"
	],
	"dependencies": {
		"classnames": "^2.3.2"
	}
}
